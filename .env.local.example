# Google AI API Key for Gemini
# Get one at: https://ai.google.dev/
GOOGLE_GENERATIVE_AI_API_KEY=your_api_key_here

# Algolia Credentials (for production Algolia integration)
# Get yours at: https://www.algolia.com/
# Application ID (found in Dashboard > API Keys)
ALGOLIA_APPLICATION_ID=your_app_id
# Admin API Key (found in Dashboard > API Keys) - needed for upload script
ALGOLIA_ADMIN_API_KEY=your_admin_key
# Search-only API Key (safe to expose) - needed for frontend search
NEXT_PUBLIC_ALGOLIA_SEARCH_KEY=your_search_key
NEXT_PUBLIC_ALGOLIA_APP_ID=your_app_id

# Algolia Agent Studio
# Agent ID for the startup roast agent (configured in Algolia Dashboard > Agent Studio)
ALGOLIA_AGENT_ID=your_agent_id
NEXT_PUBLIC_ALGOLIA_AGENT_ID=your_agent_id
